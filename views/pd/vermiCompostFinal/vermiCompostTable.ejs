<%if (records.length >0){ %>
    <table class="table table-bordered table-striped" id="tblCustomers" cellspacing="0" cellpadding="0">
    <thead>
    <tr>
        <th>ক্রমিক নং</th>
        <th>কৃষকের নাম ও পিতার নাম</th>
        <th>মোবাইল নম্বর ও জাতীয় পরিচয়পত্র নম্বর</th>
        <th>জাতের নাম</th>
        <th>বীজতলায় বপনের তারিখ</th>
        <th>কর্তনের তারিখ</th>
        <th>হেক্টর ফলন</th>
        <th>সংরক্ষিত বীজের পরিমাণ</th>
        <th>মন্তব্য</th>
    </tr>
    </thead>
    <tbody>
        <%console.log("records",records) %>
        <% records.forEach(function(row,index) { %>
            <tr>
                <td><%= index+1 %></td>
                <td><%= row.name %></td>
                <td><%= row.mobile %></td>
                <td><%= row.breed %></td>
                <td><%= row.bdate %></td>
                <td><%= row.kdate %></td>
                <td><%= row.folon %></td>
                <td><%= row.bij %></td>
                <td><%= row.comment %></td>
            </tr>
        <% }) %>
    </tbody>
</table>
<% }else{ %>
    <table class="table table-bordered table-striped" id="tblCustomers" cellspacing="0" cellpadding="0">
        <thead>
        <tr>
            <th>ক্রমিক নং</th>
        <th>কৃষকের নাম ও পিতার নাম</th>
        <th>মোবাইল নম্বর ও জাতীয় পরিচয়পত্র নম্বর</th>
        <th>জাতের নাম</th>
        <th>বীজতলায় বপনের তারিখ</th>
        <th>কর্তনের তারিখ</th>
        <th>হেক্টর ফলন</th>
        <th>সংরক্ষিত বীজের পরিমাণ</th>
        <th>মন্তব্য</th>
        </tr>
        </thead>
        <tbody>
        <% records.forEach(function(row){ %>
            <tr>
                <td colspan="10">No data Found</td>
            </tr>
        <% }) %>
        </tbody>
    </table>
<% } %>