<%- include('../../includes/header')-%>
        <div id="wrapper" class="wrapper" >
            <%- include('../../includes/topbar')-%>
            <div class="page-wrap">
                <%- include('../../includes/sidebar')-%>
                <div class="main-content">
                    <div class="container-fluid">
                        <div class="row clearfix">
                            <div class="col-md-10">
                                <h1 class="text-center"><%= title %></h1>
                            </div>
                        </div>
                        <div class="row mb-2">
                            <div class="col-md-12">
                                <a href="/upazilla/progressForm" class="btn btn-success btn-group-sm ">চলমান কার্যক্রমের অগ্রগতি ফর্ম</a>
                            </div>

                        </div>

                        <div class="table-responsive" id="output">
                            <%if (records.length >0){ %>
                                <table class="table table-bordered table-striped" id="" cellspacing="0" cellpadding="0">
                                <thead>
                                <tr>
                                    <th rowspan="2">ক্রমিক নং</th>
                                    <th colspan="2">নির্মাণ কাজের অগ্রগতি</th>
                                    <th colspan="2">প্রশিক্ষণ কার্যক্রমের অগ্রগতি</th>
                                    <th colspan="2">প্রদর্শনী কার্যক্রম</th>
                                    <th rowspan="2">গাড়ী</th>
                                    <th rowspan="2">পরিবর্তন/মুছুন</th>
                                </tr>
                                <tr>
                                    <th>বর্তমান অবস্থা</th>
                                    <th>পর্যবেক্ষণ</th>
                                    <th>বর্তমান অবস্থা</th>
                                    <th>পর্যবেক্ষণ</th>
                                    <th>বর্তমান অবস্থা</th>
                                    <th>পর্যবেক্ষণ</th>
                                </tr>
                                </thead>
                                <tbody>
                                    
                                    <% records.forEach(function(row,index) { %>
                                        <tr>
                                            <td><%= index+1 %></td>
                                            <td><%= row.nirmanPresent %></td>
                                            <td><%= row.nirmanComment %></td>
                                            <td><%= row.proshikkhonPresent %></td>
                                            <td><%= row.proshikkhonComment %></td>
                                            <td><%= row.prodorshoniPresent %></td>
                                            <td><%= row.prodorshoniComment %></td>
                                            <td><%= row.car %></td>
                                            <td><a class="btn btn-primary btn-xs" href="/upazilla/progressFormEdit/<%= row.id %>"><i class="fa fa-edit"></i></a>
                                                <a class="btn btn-danger btn-xs" href="/upazilla/progressDelete/<%= row.id %>"><i class="fa fa-trash"></i></a>
                                            </td>
                                        </tr>
                                    <% }) %>
                                </tbody>
                            </table>
                            <% }else{ %>
                                <table class="table table-bordered table-striped" id="tblCustomers" cellspacing="0" cellpadding="0">
                                    <thead>
                                        <tr>
                                            <th rowspan="2">ক্রমিক নং</th>
                                            <th colspan="2">নির্মাণ কাজের অগ্রগতি</th>
                                            <th colspan="2">প্রশিক্ষণ কার্যক্রমের অগ্রগতি</th>
                                            <th colspan="2">প্রদর্শনী কার্যক্রম</th>
                                            <th rowspan="2">গাড়ী</th>
                                        </tr>
                                        <tr>
                                            <th>বর্তমান অবস্থা</th>
                                            <th>পর্যবেক্ষণ</th>
                                            <th>বর্তমান অবস্থা</th>
                                            <th>পর্যবেক্ষণ</th>
                                            <th>বর্তমান অবস্থা</th>
                                            <th>পর্যবেক্ষণ</th>
                                        </tr>
                                        </thead>
                                    <tbody>
                                    <% records.forEach(function(row){ %>
                                        <tr>
                                            <td colspan="10">No data Found</td>
                                        </tr>
                                    <% }) %>
                                    </tbody>
                                </table>
                            <% } %>
                        </div>


                        </div>
                </div>
            </div>
        </div>


   
<%- include('../../includes/footer')-%>
