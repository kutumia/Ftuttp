<%- include('../../includes/header')-%>
        <div class="wrapper">
          <%- include('../../includes/topbar')-%>
            <div class="page-wrap">
              <%- include('../../includes/sidebar')-%>
              <div class="main-content">
                <div class="container-fluid">
                    <div class="row clearfix">
                    <div id="signupbox" style="margin-top:50px" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <div class="panel-title"><h2>চূড়ান্ত প্রতিবেদন ফর্ম</h2></div>
                                <p><%
                                if(success !=''){ %>
                                <%= success %>
                                    <% } %>
                                </p>
                                </div>  
                            <div class="panel-body" >
                                <form action="/upazilla/demonstrationFinalFormEditPost/<%= records.id %>" id="FieldTrialForm" class="form-horizontal" method="post"  role="form">
                                    <div id="signupalert" style="display:none" class="alert alert-danger">
                                        <p>Error:</p>
                                        <span></span>
                                    </div>

                                    <div class="form-group">
                                        <label for="name" class="col-md-3 control-label">কৃষকের নাম ও পিতার নাম</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="name" placeholder="কৃষকের নাম ও পিতার নাম" value="<%=records.name%>">
                                        </div>
                                    </div>


                                    <div class="form-group">
                                        <label for="mobile" class="col-md-3 control-label">মোবাইল নম্বর ও জাতীয় পরিচয়পত্র নম্বর</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="mobile" placeholder="মোবাইল নম্বর ও জাতীয় পরিচয়পত্র নম্বর" value="<%=records.mobile%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="breed" class="col-md-3 control-label">জাতের নাম</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="breed" placeholder="জাতের নাম" value="<%=records.breed%>">
                                        </div>
                                    </div>


                                    <div class="form-group">
                                        <label for="bdate" class="col-md-3 control-label">বীজতলায় বপনের তারিখ</label>
                                        <div class="col-md-9">
                                            <input type="date" class="form-control" name="bdate" placeholder="বীজতলায় বপনের তারিখ" value="<%=records.bdate%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="kdate" class="col-md-3 control-label">কর্তনের তারিখ</label>
                                        <div class="col-md-9">
                                            <input type="date" class="form-control" name="kdate" placeholder="কর্তনের তারিখ" value="<%=records.kdate%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="folon" class="col-md-3 control-label">হেক্টর ফলন</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="folon" placeholder="হেক্টর ফলন" value="<%=records.folon%>">
                                        </div>
                                    </div>


                                    <div class="form-group">
                                        <label for="bij" class="col-md-3 control-label">সংরক্ষিত বীজের পরিমাণ</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="bij" placeholder="সংরক্ষিত বীজের পরিমাণ" value="<%=records.bij%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="comment" class="col-md-3 control-label">মন্তব্য</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="comment" placeholder="মন্তব্য" value="<%=records.comment%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div class="col-md-9">
                                            <input type="hidden" class="form-control" name="user_id" placeholder="id" value="<%= user_id %>">
                                        </div>
                                    </div>
                                    
                                    <div class="form-group">
                                        <!-- Button -->                                        
                                        <div class="col-md-offset-3 col-md-9">
                                        <input type="submit" name="submit" value="submit" class="btn btn-primary">   
                                        </div>
                                    </div>
                                    
                                    
                                        
                                    
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<%- include('../../includes/footer')-%>